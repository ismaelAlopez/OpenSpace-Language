local earth = asset.require("scene/solarsystem/planets/earth/earth")
local data = asset.require("../annular_data")



local Layer = {
  Identifier = "Annular-2030-06-01",
  Enabled = false,
  File = data.path .. "2030-06-01.geojson",
  HeightOffset = 2000.0,
  DefaultProperties = {
    LineWidth = 2.0,
    Extrude = true
  }
}


asset.onInitialize(function()
  openspace.globebrowsing.addGeoJson(earth.Earth.Identifier, Layer)
end)

asset.onDeinitialize(function()
  openspace.globebrowsing.deleteGeoJson(earth.Earth.Identifier, Layer)
end)



asset.meta = {
  Name = "Anular 2030-06-01",
  Description = "Mapa de Eclipse Anular para 2030-06-01",
  Author = "Equipo de OpenSpace",
  URL = "http://openspaceproject.com",
  License = "MIT license"
}
