asset.onInitialize(function()
  openspace.addSceneGraphNode(Albiorix)
  openspace.addSceneGraphNode(AlbiorixTrail)
  openspace.addSceneGraphNode(AlbiorixLabel)

  openspace.addSceneGraphNode(Bebhionn)
  openspace.addSceneGraphNode(BebhionnTrail)
  openspace.addSceneGraphNode(BebhionnLabel)

  openspace.addSceneGraphNode(Erriapus)
  openspace.addSceneGraphNode(ErriapusTrail)
  openspace.addSceneGraphNode(ErriapusLabel)

  openspace.addSceneGraphNode(Tarvos)
  openspace.addSceneGraphNode(TarvosTrail)
  openspace.addSceneGraphNode(TarvosLabel)
end)

asset.onDeinitialize(function()
  openspace.removeSceneGraphNode(TarvosLabel)
  openspace.removeSceneGraphNode(TarvosTrail)
  openspace.removeSceneGraphNode(Tarvos)

  openspace.removeSceneGraphNode(ErriapusLabel)
  openspace.removeSceneGraphNode(ErriapusTrail)
  openspace.removeSceneGraphNode(Erriapus)

  openspace.removeSceneGraphNode(BebhionnLabel)
  openspace.removeSceneGraphNode(BebhionnTrail)
  openspace.removeSceneGraphNode(Bebhionn)

  openspace.removeSceneGraphNode(AlbiorixLabel)
  openspace.removeSceneGraphNode(AlbiorixTrail)
  openspace.removeSceneGraphNode(Albiorix)
end)

